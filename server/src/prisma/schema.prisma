generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "windows"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model History {
  id           Int      @id @default(autoincrement())
  nrInst       Int
  nrAgencia    Int
  cdClient     Int
  nmClient     String
  nrCpfCnpj    String?
  nrContrato   Int
  dtContrato   DateTime
  qtPrestacoes Int
  vlTotal      String
  cdProduto    Int
  dsProduto    String
  cdCarteira   Int
  dsCarteira   String
  nrProposta   Int
  nrPresta     Int
  tpPresta     String
  nrSeqPre     Int
  dtVctPre     DateTime
  vlMora       String
  vlMulta      String
  vlOutAcr     String
  vlIof        String
  vlAtual      String
  vlDescon     String
  vlPresta     String
  idSituac     String
  idSitVen     String
}
